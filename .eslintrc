{
  "extends": "airbnb-base",
  "env": {
    "node": true,
    "mocha": true
  },
  "rules": {
    "func-names": 0,
    "no-multi-spaces": 0,
    "no-console": 0,
    "no-unused-expressions": 0,
    "space-before-function-paren": 0,
    "comma-dangle": 0,
    "space-after-keywords": 0,
    "keyword-spacing": 0,
    "object-curly-spacing":0,
    "no-param-reassign": 0
  },
  "globals": {
    "global": true,
    "angular": true,
    "jquery": true,
    "$": true,
    "_": true,
    "require": true
  }
}
